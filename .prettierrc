{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 100,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "endOfLine": "lf",
  "embeddedLanguageFormatting": "auto",
  "useTabs": false,
  "bracketSpacing": true,
  "autoCloseBrackets": true,
  "autoCloseTags": true,
  "scrollBeyondLastLine": false
}
